{
  "expo": {
    "name": "catfish",
    "slug": "catfish",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "Access for camera to pass photo/video identification",
        "NSPhotoLibraryUsageDescription": "Access to save analyzed images to your photo library",
        "NSPhotoLibraryAddUsageDescription": "Access to save analyzed images to your photo library"
      },
      "bundleIdentifier": "com.anonymous.catfish"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.catfish",
      "edgeToEdgeEnabled": true,
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "READ_MEDIA_IMAGES"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "cbf78e18-4448-45c9-9bb2-c9e85e184882"
      }
    }
  }
}
